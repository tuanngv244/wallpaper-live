{
  "name": "wallpaper-live",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "check:eslint": "eslint \"src/**/*.{vue,js,ts}\"",
    "check:types": "vue-tsc --noEmit",
    "check:biome": "biome check",
    "check": "run-p check:*",
    "fmt:eslint": "eslint \"src/**/*.{vue,js,ts}\" --fix",
    "fmt:biome": "biome check --write",
    "fmt:translation": "ts-node ./scripts/translation-lint.ts",
    "fmt:graphql": "ts-node ./scripts/graphql-lint.ts",
    "fmt:yml": "eslint .github/workflows/**/*.{yml,yaml} -c .github/eslint.config.mjs",
    "fmt:cwebp": "sh ./scripts/cwebp.sh --path=./",
    "fmt": "run-p fmt:*",
    "test": "vitest run",
    "codegen:openapi": "openapi-typescript src/lib/openapi/openapi.yaml -o src/lib/openapi/openapi.ts",
    "codegen:graphql": "graphql-codegen",
    "codegen": "run-p codegen:*",
    "changeset:publish": "pnpm build && pnpm changeset publish",
    "changeset": "changeset",
    "depcheck": "knip --dependencies",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.1",
    "@tailwindcss/postcss": "^4.1.14",
    "autoprefixer": "^10.4.21",
    "pinia": "^3.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "biome": "^0.3.3",
    "eslint": "9",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-plugin-yml": "^1.19.0",
    "graphql-codegen": "^0.4.0",
    "knip": "^5.67.0",
    "openapi-typescript": "^7.10.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.6",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}